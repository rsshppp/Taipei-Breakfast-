<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="model.bean.AdvertisementStatusBean" table="AdvertisementStatus">
        <id name="advertisementStatusID" type="java.lang.Integer">
            <column name="advertisementStatusID" />
            <generator class="assigned" />
        </id>
        <property name="advertisementStatus" type="java.lang.String">
            <column name="advertisementStatus" />
        </property>
    </class>
</hibernate-mapping>