<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="model.bean.AccuseStatusBean" table="AccuseStatus">
        <id name="accuseStatusID" type="java.lang.Integer">
            <column name="accuseStatusID" />
            <generator class="assigned" />
        </id>
        <property name="accuseStatus" type="java.lang.String">
            <column name="accuseStatus" />
        </property>
    </class>
</hibernate-mapping>